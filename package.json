{
  "name": "active-time",
  "description": "A foundation for scalable desktop apps",
  "keywords": [
    "active time"
  ],
  "homepage": "https://github.com/Jannchie/active-time#readme",
  "bugs": {
    "url": "https://github.com/Jannchie/active-time/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jannchie/active-time.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jannchie",
    "email": "jannchie@gmail.com"
  },
  "main": "./src/main/main.cjs",
  "scripts": {
    "build": "concurrently \"pnpm run build:main\" \"pnpm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "pnpm exec vite build",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "package": "ts-node ./.erb/scripts/clean.js dist && pnpm run build && electron-builder build --publish never",
    "prepare": "husky install",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && concurrently \"pnpm run start:main\" \"pnpm run start:preload\" \"pnpm run start:renderer\"",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "pnpm exec vite",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@nuxt/ui": "^4.3.0",
    "@vueuse/core": "^14.1.0",
    "drizzle-orm": "^0.45.1",
    "electron-debug": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "iconv-lite": "^0.7.1",
    "node-cron": "^4.2.1",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@electron/rebuild": "^4.0.2",
    "@jannchie/eslint-config": "^3.7.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.23",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^5.6.2",
    "concurrently": "^9.2.1",
    "core-js": "^3.47.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "detect-port": "^2.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electronmon": "^2.0.4",
    "eslint": "9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.5",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "sass": "^1.97.1",
    "sass-loader": "^16.0.6",
    "source-map-support": "^0.5.21",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "terser-webpack-plugin": "^5.3.16",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "vite": "^7.3.0",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "build": {
    "productName": "ActiveTime",
    "appId": "com.jannchie.ActiveTime",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "jannchie",
      "repo": "active-time"
    }
  },
  "engines": {
    "node": ">=18.18",
    "pnpm": ">=10.0.0"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/*"
    ],
    "logLevel": "quiet"
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6"
}
